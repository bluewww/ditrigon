<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>

  <object class="GtkWindow" id="rawlog_window">
    <property name="default-width">720</property>
    <property name="default-height">420</property>
    <property name="child">
      <object class="GtkBox" id="rawlog_root">
        <property name="orientation">vertical</property>
        <property name="spacing">8</property>
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="margin-top">12</property>
        <property name="margin-bottom">12</property>
        <child>
          <object class="GtkScrolledWindow" id="rawlog_scroll">
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <property name="child">
              <object class="GtkTextView" id="rawlog_text_view">
                <property name="editable">False</property>
                <property name="cursor-visible">False</property>
                <property name="monospace">True</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="rawlog_buttons">
            <property name="orientation">horizontal</property>
            <property name="spacing">6</property>
            <property name="halign">end</property>
            <child>
              <object class="GtkButton" id="rawlog_clear_button"/>
            </child>
            <child>
              <object class="GtkButton" id="rawlog_save_button"/>
            </child>
            <child>
              <object class="GtkButton" id="rawlog_close_button"/>
            </child>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
